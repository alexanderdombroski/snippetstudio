  "show-deprecation-stack": {
    "prefix": "verbose-logging",
    "body": [
      "if (process.env.USE_VERBOSE_LOGGING === 'true') {",
      "\tprocess.on('warning', (warning) => {",
      "\t\tif (warning.name === 'DeprecationWarning' || warning.name === 'ExperimentalWarning') {",
      "\t\t\tconsole.error(warning.stack);",
      "\t\t}",
      "\t});",
      "}"
    ],
    "description": "enable verbose logging for extension troubleshooting",
    "scope": "typescript"
  }
